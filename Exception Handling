package net.codejava;

public class ExceptionHandling 
{

	public static void main(String[] args) 
	{
		try
		{
			int a = 10/0;
			System.out.println("Hi");
		}
         catch(ArithmeticException e)
		{
    	   System.out.println(e);
    	   System.out.println("can't divide by zero");
		}
	    finally
		{
			System.out.println("Hi");
		}
	}		
}

