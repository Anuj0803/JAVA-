package net.codejava;

public class string 
{

	public static void main(String[] args) 
	{
		char[] ch={'H','e','l','l','o'};  
		String s=new String(ch); 
		String g = new String("hello");
		String d = "hello caelius";
		String x =d.concat(" "+g);
		String h = "CAELIUS "+"CONSULTING";
		String p ="caelius consulting";
		String i = " caelius ";
		System.out.println(g.equals(s));
		System.out.println(d);
		System.out.println(g);
		System.out.println(x);
		System.out.println(h);
		System.out.println(h.substring(6));
		System.out.println(h.substring(0,4));
		System.out.println(h.toLowerCase());
		System.out.println(p.toUpperCase());
		System.out.println(i);
		System.out.println(i.trim());
		System.out.println(p.startsWith("an"));
		System.out.println(p.endsWith("x"));
		System.out.println(p.charAt(2));
		System.out.println(p.length());
		System.out.println(p.replace("caelius", "CAELIUS"));
		System.out.println(d.contains("el"));
	}
}
